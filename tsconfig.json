{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "jsx": "react-jsx",
    "allowJs": true,
    "target": "esnext",
    "module": "commonjs",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "strictBindCallApply": true,
    "strict": true,
    "types": ["axe-playwright"]
  },
  "include": ["src", ".storybook"]
}
